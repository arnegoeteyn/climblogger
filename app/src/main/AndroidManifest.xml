<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
        package="com.example.climblogger">

    <application
            android:allowBackup="true"
            android:icon="@mipmap/ic_launcher"
            android:label="@string/app_name"
            android:roundIcon="@mipmap/ic_launcher_round"
            android:supportsRtl="true"
            android:theme="@style/AppTheme">
        <activity android:name=".ui.multipitch.MultipitchesActivity"></activity>
        <activity android:name=".ui.route.EditRouteActivity" />
        <activity android:name=".ui.area.EditAreaActivity" />
        <activity android:name=".ui.sector.EditSectorActivity" />
        <activity android:name=".ui.stats.StatsActivity" />
        <activity android:name=".ui.area.AreaActivity" />
        <activity android:name=".ui.sector.SectorActivity" />
        <activity android:name=".ui.area.AddAreaActivity" />
        <activity android:name=".ui.sector.AddSectorActivity" />
        <activity android:name=".ui.route.AddRouteActivity" />
        <activity android:name=".ui.ascent.AscentActivity" />
        <activity android:name=".ui.ascent.EditAscentActivity" />
        <activity android:name=".ui.ascent.AddAscentActivity" />
        <activity android:name=".ui.route.RouteActivity" />
        <activity
                android:name=".ui.main.MainActivity"
                android:label="@string/app_name"
                android:theme="@style/AppTheme.NoActionBar">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>